FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/ARTableViewManager" "$PODS_CONFIGURATION_BUILD_DIR/REFormattedNumberField" "$PODS_CONFIGURATION_BUILD_DIR/REValidation"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_CONFIGURATION_BUILD_DIR/ARTableViewManager/ARTableViewManager.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/REFormattedNumberField/REFormattedNumberField.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/REValidation/REValidation.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "ARTableViewManager" -framework "REFormattedNumberField" -framework "REValidation"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
